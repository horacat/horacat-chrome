{
  "name": "horacat",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "simple time tracking for github",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options_custom/index.html",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/issues*",
        "https://github.com/*/pull*"
      ],
      "js": [
        "js/debug/debug.js",
        "js/jquery/jquery.min.js",
        "js/jssha/sha1.js",
        "src/inject/inject.js"
      ]
    }
  ]
}